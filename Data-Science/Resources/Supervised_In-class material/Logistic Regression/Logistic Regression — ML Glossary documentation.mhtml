<span class=3D"n">scikit_log_reg</span><span class=3D"o">.</span><span clas=
s=3D"n">fit</span><span class=3D"p">(</span><span class=3D"n">features_trai=
n</span><span class=3D"p">,</span><span class=3D"n">labels_train</span><spa=
n class=3D"p">)</span>

<span class=3D"c1">#Score is Mean Accuracy</span>
<span class=3D"n">scikit_score</span> <span class=3D"o">=3D</span> <span cl=
