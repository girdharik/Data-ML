><span class=3D"p">(</span><span class=3D"n">features_test</span><span clas=
s=3D"p">,</span><span class=3D"n">labels_test</span><span class=3D"p">)</sp=
an>
<span class=3D"nb">print</span> <span class=3D"s1">'Scikit score: '</span><=
span class=3D"p">,</span> <span class=3D"n">scikit_score</span>

<span class=3D"c1">#Our Mean Accuracy</span>
<span class=3D"n">observations</span><span class=3D"p">,</span> <span class=
=3D"n">features</span><span class=3D"p">,</span> <span class=3D"n">labels</=
span><span class=3D"p">,</span> <span class=3D"n">weights</span> <span clas=
s=3D"o">=3D</span> <span class=3D"n">run</span><span class=3D"p">()</span>
<span class=3D"n">probabilities</span> <span class=3D"o">=3D</span> <span c=
